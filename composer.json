{
    "config": {
        "vendor-dir": "wp-content/vendor"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wp-block-admin",
                "type": "wordpress-plugin",
                "version": "0.2.3",
                "dist": {
                    "type": "zip",
                    "url": "https://downloads.wordpress.org/plugin/wp-block-admin.0.2.3.0.zip"
                },
                "require": {
                    "composer/installers": "v1.0.7"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "restrict-taxonomies",
                "type": "wordpress-plugin",
                "version": "1.3.0",
                "dist": {
                    "type": "zip",
                    "url": "https://downloads.wordpress.org/plugin/restrict-taxonomies.1.3.0.zip"
                },
                "require": {
                    "composer/installers": "v1.0.7"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "rss-shortcode",
                "type": "wordpress-plugin",
                "version": "0.2",
                "dist": {
                    "type": "zip",
                    "url": "https://downloads.wordpress.org/plugin/rss-shortcode.0.2.zip"
                },
                "require": {
                    "composer/installers": "v1.0.7"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "polylang",
                "type": "wordpress-plugin",
                "version": "1.9",
                "dist": {
                    "type": "zip",
                    "url": "https://downloads.wordpress.org/plugin/polylang.1.9.zip"
                },
                "require": {
                    "composer/installers": "v1.0.7"
                }
            }
        }
    ],
    "require-dev": {
    },
    "require": {
        "wp-block-admin": "0.2.3",
        "bibcnrs/wp-ebsco-widget": "dev-master",
        "timber/timber": "0.22.1",
        "restrict-taxonomies": "1.3.0",
        "rss-shortcode": "0.2",
        "polylang": "1.9"
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
