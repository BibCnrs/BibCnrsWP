{% extends 'category.twig' %}
{% block content %}
<div id="content" class="bsbb">
    {% block breadcrumb %}
    {{parent()}} >> {{post.title}}
    {% endblock %}
    <br class="clear"/>
    <div id="center">
        <div id="pagenorm">
            <div id="titrerubnorm2">
                <h1 class="font-{{currentDomain}}">{{post.title}}</h1>
            </div>
            <div id="retour">
                <a href="{{url}}/category/{{currentDomain}}" class="font-{{currentDomain}}">
                    Voir toutes les actualit&eacute;s de {{currentDomain}}
                </a>
            </div>
            {{post.content}}
        </div>
        {% block sidebar %}
            {% if userDomain == 'visit' or visit %}
                {% include 'sidebar-right-visit.twig' %}
            {% else %}
                {% include 'sidebar-right.twig' %}
            {% endif %}
        {% endblock %}
    </div>
</div>
{% endblock %}
