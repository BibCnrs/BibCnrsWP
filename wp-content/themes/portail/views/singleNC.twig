{% extends 'category.twig' %}
{% block title %}
    {{site.name}} - {{post.title}}
{% endblock %}
{% block content %}
<div id="content" class="bsbb">
{% block breadcrumb %}
    <span class="bread"><a href="{{site.url}}">{{__('Home', 'bibcnrs')}}</a> >> {{post.title}}</span>
{% endblock %}
    <br class="clear"/>
    <div id="center">
        {{ ebsco_widget|shortcodes }}
        <div id="pagenorm">
            <div style="position:relative; width:inherit; height:inherit;">
            </div>
            <div id="titrerubnorm2">
                <h1 class="font-{{currentCategory.description}} wph1">{{post.title}}</h1>
            </div>
            <div id="retour">
                <a href="{{referer}}" class="font-visit">{{__('Go back', 'bibcnrs')}}</a>
            </div>
            {{post.content}}
        </div>
        {% block sidebar %}
        {% endblock %}
    </div>
</div>
{% endblock %}
